{
  "name": "cdif",
  "version": "0.2.21",
  "description": "Common device interconnect framework",
  "bin": "./bin/cdif",
  "main": "./bin/framework.js",
  "scripts": {
    "start": "./bin/cdif",
    "start-allow-discover": "NODE_PATH=./lib node ./framework.js --allowDiscover",
    "start-heap-dump": "NODE_PATH=./lib node --expose-gc ./framework.js --heapDump",
    "test": "mocha"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/out4b/cdif.git"
  },
  "keywords": [
    "smart home",
    "IoT",
    "Bluetooth",
    "ONVIF",
    "Z-Wave",
    "OAuth",
    "SOA",
    "WSDL"
  ],
  "author": "out4b",
  "license": "Apache-2.0",
  "publishConfig": {
    "registry": "http://registry.apemesh.com:8000/"
  },
  "dependencies": {
    "ajv": "^4.8.2",
    "body-parser": "~1.14.0",
    "bunyan": "^1.8.1",
    "express": "~4.13.3",
    "json-schema-ref-parser": "^1.4.0",
    "jsonwebtoken": "~5.4.0",
    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1",
    "morgan": "~1.6.1",
    "npm-registry-client": "^7.2.1",
    "oauth": "^0.9.14",
    "once": "^1.4.0",
    "rewire": "^2.5.2",
    "semver": "^5.3.0",
    "socket.io": "~1.5.0",
    "sqlite3": "~3.1.0",
    "tar": "^2.2.1",
    "uuid-1345": "^0.99.6",
    "ws": "^1.0.1"
  },
  "devDependencies": {
    "async": "~1.5.0",
    "json-schema-faker": "^0.2.5",
    "should": "~7.1.0",
    "socket.io-client": "^1.4.5",
    "supertest": "~1.1.0"
  }
}
